<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:dubbo="http://dubbo.apache.org/schema/dubbo"
        xmlns="http://www.springframework.org/schema/beans"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://dubbo.apache.org/schema/dubbo
        http://dubbo.apache.org/schema/dubbo/dubbo.xsd"
>
    <dubbo:application name="judge-node">
        <dubbo:parameter key="qos.enable" value="false"/>
    </dubbo:application>

    <!-- zookeeper 注册中心 -->
    <dubbo:registry address="${dubbo.registry.zookeeper.address}" timeout="${dubbo.registry.zookeeper.timeout}"/>

    <!-- dubbo 协议 -->
    <dubbo:protocol
            name="dubbo" port="${dubbo.protocol.dubbo.port}" host="${dubbo.protocol.dubbo.host}"
            dispatcher="message" threadpool="fixed" threads="100"
    />
    <!-- hessian协议 -->
    <dubbo:protocol name="hessian" port="${dubbo.protocol.hessian.port}" server="jetty"/>

    <!-- 默认使用dubbo协议 -->
    <dubbo:provider protocol="dubbo"/>

    <!-- 向注册中心注册暴露服务地址,注册服务 -->
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.AnalyserInfoMaintainService"
            ref="analyserInfoMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.AnalyserSupportMaintainService"
            ref="analyserSupportMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.AnalyserVariableMaintainService"
            ref="analyserVariableMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.AnalysisMaintainService"
            ref="analysisMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.DriverInfoMaintainService"
            ref="driverInfoMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.DriverSupportMaintainService"
            ref="driverSupportMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.JudgerInfoMaintainService"
            ref="judgerInfoMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.JudgerSupportMaintainService"
            ref="judgerSupportMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.JudgerVariableMaintainService"
            ref="judgerVariableMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.TaskMaintainService"
            ref="taskMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.TaskEventMaintainService"
            ref="taskEventMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.SectionMaintainService"
            ref="sectionMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.AnalysisFileInfoMaintainService"
            ref="analysisFileInfoMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.AnalysisFileFileOperateService"
            ref="analysisFileFileOperateServiceImpl"
            protocol="hessian"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.AnalysisFilePackMaintainService"
            ref="analysisFilePackMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.AnalysisFilePackItemInfoMaintainService"
            ref="analysisFilePackItemInfoMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.AnalysisFilePackItemFileOperateService"
            ref="analysisFilePackItemFileOperateServiceImpl"
            protocol="hessian"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.AnalysisPictureInfoMaintainService"
            ref="analysisPictureInfoMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.AnalysisPictureFileOperateService"
            ref="analysisPictureFileOperateServiceImpl"
            protocol="hessian"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.AnalysisPicturePackMaintainService"
            ref="analysisPicturePackMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.AnalysisPicturePackItemInfoMaintainService"
            ref="analysisPicturePackItemInfoMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.AnalysisPicturePackItemFileOperateService"
            ref="analysisPicturePackItemFileOperateServiceImpl"
            protocol="hessian"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.SinkerInfoMaintainService"
            ref="sinkerInfoMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.SinkerMetaIndicatorMaintainService"
            ref="sinkerMetaIndicatorMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.SinkerMetaMaintainService"
            ref="sinkerMetaMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.SinkerRelationMaintainService"
            ref="sinkerRelationMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.SinkerSupportMaintainService"
            ref="sinkerSupportMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.SinkerVariableMaintainService"
            ref="sinkerVariableMaintainServiceImpl"
            group="${dubbo.provider.group}"
    />
    <dubbo:service
            interface="com.dwarfeng.judge.stack.service.SinkerMetaOperateService"
            ref="sinkerMetaOperateServiceImpl"
            group="${dubbo.provider.group}"
    />

    <!-- 向注册中心注册暴露服务地址,注册消费者服务 -->
    <dubbo:reference
            id="snowflakeGenerateService"
            interface="com.dwarfeng.sfds.stack.service.GenerateService"
            check="false"
            group="${dubbo.consumer.snowflake.group}"
    />
</beans>
